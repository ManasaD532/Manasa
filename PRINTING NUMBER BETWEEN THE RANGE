int a,b,c;
    printf("enter lower range:");
    scanf("%d",&a);
    printf("enter upper range:");
    scanf("%d",&b);
    for(int i=a;i<=b;i++)
    {
        c=0;
        for(int j=2;j<=a;j++)
        {
            if(i%j==0)
            {
                c+=1;
        }
        }
    if(c==0)
        {
            printf(" %d",i);
        }
    }
}
    
